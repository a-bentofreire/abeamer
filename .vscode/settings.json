{
    "tslint.exclude": "./test/tests/vendor/*.ts",
    "search.exclude": {
        "release/**": true,
        "dev-docs/js/**": true,
        "node_modules/**": true,
        "client/lib/js/*.js": true,
        "server/*.js": true,
    },
    "liveServer.settings.ignoreFiles": [
        ".vscode/**",
        "**/*.scss",
        "**/*.sass",
        "docs/**",
        "**/story-frames/*",
        "**/story.json"
    ],
    "cSpell.enabledLanguageIds": [
      "c",
      "cpp",
      "csharp",
      "css",
      "go",
      "handlebars",
      "html",
      "ini",
      "jade",
      "javascript",
      "javascriptreact",
      "json",
      "latex",
      "less",
      "markdown",
      "php",
      "plaintext",
      "pub",
      "python",
      "restructuredtext",
      "scss",
      "text",
      "typescript",
      "typescriptreact",
      "yml"
    ],
    "cSpell.words": [
        "Adpt",
        "Alexandre",
        "Animes",
        "Beamer",
        "Bento",
        "Freire",
        "Frmt",
        "Msgs",
        "NEFs",
        "OUTDIR",
        "PROJDIR",
        "Polyline",
        "Roadmap",
        "Serverless",
        "Slimer",
        "TNEF",
        "abeamer",
        "abslf",
        "concats",
        "consts",
        "darkgrey",
        "dsts",
        "easings",
        "fdeg",
        "framecount",
        "framespattern",
        "framespatterns",
        "gifs",
        "gitignore",
        "gulpfile",
        "inparsers",
        "iphone",
        "islocal",
        "loglevel",
        "mkdocs",
        "nbsp",
        "npmignore",
        "pels",
        "pixabay",
        "preservetime",
        "repo",
        "repos",
        "rgba",
        "slimerjs",
        "sprintf",
        "srcs",
        "stylesheets",
        "teleportable",
        "teleporter",
        "teleporting",
        "toolset",
        "transf",
        "typedoc",
        "uncomment",
        "uuid",
        "validators",
        "webm"
    ],
    "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/**": true,
      "**/private/**": true,
      "**/*.js.map": true,
      "**/website/**": true,
      "**/story-frames/*.png": true,
      "**/test/tests.run/**": true,
      "**/gallery-release/**": true,
      "**/release/**": true,
      "**/docs/build/**": true,
      "**/*.bak.*": true,
    },
}