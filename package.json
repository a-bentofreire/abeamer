    "@types/gulp": "^4.0.5",
    "@types/jquery": "^3.3.8",
    "@types/js-yaml": "^3.11.2",
    "@types/marked": "^0.3.0",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.6.31",
    "@types/puppeteer": "^1.6.4",
    "@types/rimraf": "^2.0.2",
    "@types/slimerjs": "^0.10.2",
    "chai": "^4.1.2",
    "font-awesome": "^4.7.0",
    "globule": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-exec": "^3.0.2",
    "gulp-minify": "^2.1.0",
    "gulp-preservetime": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^0.6.1",
    "gulp-sequence": "^1.0.0",
    "gulp-zip": "^4.2.0",
    "happner-serial-mocha": "^1.5.2",
    "highlight.js": "^9.12.0",
    "js-yaml": "^3.12.0",
    "marked": "^0.3.19",
    "merge-stream": "^1.0.1",
    "mocha": "^5.2.0",
    "mocha-image-compare": "^1.0.3",
    "puppeteer": "^1.8.0",
    "rimraf": "^2.6.2",
    "typescript": "^2.9.2"
  },
  "optionalDependencies": {},
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "abeamer": "node ./cli/abeamer-cli.js",
    "render": "node ./cli/abeamer-cli.js render",
    "gif": "node ./cli/abeamer-cli.js gif",
    "serve": "node ./cli/abeamer-cli.js serve",
    "serve-with-dirs": "node ./cli/abeamer-cli.js serve --list-dir",
    "serve-website-www": "cd website && npm run serve-www",
    "---- start: before commands ----": "----",
    "before-commit-dev": "gulp README-to-online && gulp build-definition-files && gulp build-shared-lib && gulp bump-version",
    "---- end: before commands ----": "----",
    "mkdocs-build-developer": "cd docs/latest/developer/en && mkdocs build",
    "mkdocs-build-end-user": "cd docs/latest/end-user/en && mkdocs build",
    "build-docs-latest-and-mkdocs": "gulp build-docs-latest && npm run mkdocs-build-developer && npm run mkdocs-build-end-user && gulp post-build-docs-latest",
    "build-release-latest": "npm run before-commit-dev && gulp build-release-latest-internal",
    "---- start: render gallery commands ----": "----",
    "g:animate-colors": "npm run abeamer -- render --ll 3 --dp gallery/animate-colors",
    "g:animate-pixels": "npm run abeamer -- render --ll 3 --dp gallery/animate-pixels",
    "g:animate-counter": "npm run abeamer -- render --ll 3 --dp gallery/animate-counter",
    "g:animate-4k": "npm run abeamer -- render --ll 3 --dp gallery/animate-4k",
    "g:animate-loop": "npm run abeamer -- render --ll 3 --dp gallery/animate-loop",
    "g:animate-abeamer": "npm run abeamer -- render --ll 3 --dp gallery/animate-abeamer",
    "g:animate-animate-rec-high-res": "npm run abeamer -- render --ll 3 --dp gallery/animate-rec-high-res",
    "---- start: print commands ----": "----",
    "print-usage": "npm run abeamer",
    "print-version": "npm run abeamer -- --version",
    "---- start: test commands ----": "----",
    "test-all-serial": "node test/tests.js  --serial",
    "test-all": "mocha test/tests.js",
    "test-sanity": "mocha test/tests/test-sanity.js",
    "test-missing-selector": "mocha test/tests/test-missing-selector.js",
    "test-move-pixels": "mocha test/tests/test-move-pixels.js",
    "test-duration": "mocha test/tests/test-duration.js",
    "test-props": "mocha test/tests/test-props.js",
    "test-direction": "mocha test/tests/test-direction.js",
    "test-action-ranges": "mocha test/tests/test-action-ranges.js",
    "test-stills": "mocha test/tests/test-stills.js",
    "test-easings": "mocha test/tests/test-easings.js",
    "test-gen-story-frames": "mocha test/tests/test-gen-story-frames.js",
    "test-multiple-scenes": "mocha test/tests/test-multiple-scenes.js",
    "test-flyovers": "mocha test/tests/test-flyovers.js",
    "test-phantomjs": "mocha test/tests/test-phantomjs.js",
    "test-render-frame-options": "mocha test/tests/test-render-frame-options.js",
    "test-expressions": "mocha test/tests/test-expressions.js",
    "test-advance": "mocha test/tests/test-advance.js"
  }
}
