  "engines": {
    "node": "*"
  },
  "bin": {
    "abeamer": "cli/abeamer-cli.js"
  },
  "homepage": "https://abeamer.devtoix.com",
  "bugs": "https://github.com/a-bentofreire/abeamer/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-bentofreire/abeamer.git"
  },
  "main": "abeamer.js",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/core-js": "^0.9.46",
